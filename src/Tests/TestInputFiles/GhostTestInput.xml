<?xml version="1.0"?>
<input xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../XML_Parser/input.xsd">

    <t_end>0.0005</t_end>
    <delta_t>0.0005</delta_t>
    <calcType>LJ</calcType>
    <baseNameOutputFiles>Ghost_Test</baseNameOutputFiles>
    <writeFrequency>1</writeFrequency>
    <gravInput></gravInput>

    <paramsLJ>
        <epsilon>5.0</epsilon>
        <mass>1.0</mass>
        <rho>1.0</rho>
        <h>1.125</h>
        <domainSizeX>7</domainSizeX>
        <domainSizeY>11</domainSizeY>
        <domainSizeZ>12.5</domainSizeZ>
        <particleContainerType>2</particleContainerType>
        <cutoffRadius>3</cutoffRadius>
    </paramsLJ>

    <boundaryConditions>
        <top>2</top>
        <right>2</right>
        <bottom>2</bottom>
        <left>2</left>
        <front>2</front>
        <back>2</back>
    </boundaryConditions>

    <particlesLJ>
        <body>
            <bodyType>Cuboid</bodyType>
            <position>1,1,1</position>
            <velocity>0,0,0</velocity>
            <objectSpecificFormat>1,8,11</objectSpecificFormat>
        </body>
    </particlesLJ>

</input>
